<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.proff2.example"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="main.java.com.myapp.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="main.java.com.myapp.activity.DBExampleActivity" />
        <activity android:name="main.java.com.myapp.activity.ListActivity" />
        
    </application>

</manifest>
